<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MenuController">
   <children>
      <TextField fx:id="voters" layoutX="124.0" layoutY="144.0" text="100" AnchorPane.topAnchor="144.0" />
      <TextField fx:id="ratings" layoutX="124.0" layoutY="194.0" text="5" AnchorPane.topAnchor="194.0" />
      <TextField fx:id="parties" layoutX="124.0" layoutY="244.0" text="10" AnchorPane.topAnchor="244.0" />
      <TextField fx:id="districts" layoutX="123.0" layoutY="295.0" text="1" />
      <Button layoutX="165.0" layoutY="329.0" mnemonicParsing="false" onAction="#openCompareView" text="Generate" AnchorPane.leftAnchor="165.0" AnchorPane.rightAnchor="170.0" AnchorPane.topAnchor="329.0" />
      <Label layoutX="125.0" layoutY="120.0" prefHeight="24.0" prefWidth="149.0" text="Number of Voters" AnchorPane.leftAnchor="125.0" AnchorPane.rightAnchor="126.0" />
      <Label layoutX="127.0" layoutY="177.0" prefHeight="17.0" prefWidth="149.0" text="Number of Ratings" />
      <Label layoutX="126.0" layoutY="227.0" prefHeight="17.0" prefWidth="149.0" text="Number of Parties" />
      <ComboBox fx:id="cmbBox" layoutX="125.0" layoutY="51.0" onAction="#resetChoices" prefWidth="150.0" promptText="Choose type" />
      <ComboBox fx:id="leftBox" layoutX="49.0" layoutY="87.0" prefWidth="150.0" promptText="Left Voting System" />
      <ComboBox fx:id="rightBox" layoutX="201.0" layoutY="87.0" prefWidth="150.0" promptText="Right Voting System" />
      <Label layoutX="125.0" layoutY="278.0" prefHeight="17.0" prefWidth="149.0" text="Number of Districts" />
      <CheckBox fx:id="customDistrictBox" layoutX="276.0" layoutY="299.0" mnemonicParsing="false" onAction="#toggleCustomDistrictWindow" text="Custom Districs" />
   </children>
</AnchorPane>
